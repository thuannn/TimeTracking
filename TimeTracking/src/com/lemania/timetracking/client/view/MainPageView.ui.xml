<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:FlowPanel width="100%" height="100%">
		<g:DockPanel width="100%" height="100%" spacing="5" borderWidth="1">
			<g:Dock direction="NORTH" height="40px">
				<g:Image url="images/logo.jpg" width=""/>
			</g:Dock>
			<g:Dock direction="WEST" width="10%">
				<g:Tree animationEnabled="true">
					<g:Button ui:field="cmdHomepage">Accueil</g:Button>
					<g:Button>Mettre des heures</g:Button>
					<g:TreeItem text="Informations de base" state="true">
						<g:TreeItem text="Ecoles" state="true">
						<g:Button ui:field="cmdEcolePage">Liste d'écoles</g:Button>
						<g:Button text="Ajouter une école" ui:field="cmdEcoleAdd"/>
					</g:TreeItem>
					</g:TreeItem>
					<g:Button>Contact</g:Button>
				</g:Tree>
			</g:Dock>
			<g:Dock direction="CENTER">
				<g:FlowPanel ui:field="mainContentPanel" width="100%" height="100%"/>
			</g:Dock>
		</g:DockPanel>
	</g:FlowPanel>
</ui:UiBinder>
