<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:FlowPanel width="100%" height="100%">
		<g:DockPanel width="100%" height="100%" spacing="5" ui:field="dockPanel">
			<g:Dock direction="NORTH">
				<g:HorizontalPanel verticalAlignment="ALIGN_BOTTOM" width="100%" height="54px" spacing="5">
					<g:Image url="images/logo.jpg" width=""/>
					<g:Cell verticalAlignment="ALIGN_BOTTOM">
						<g:Grid height="" width="600px" cellPadding="3" cellSpacing="2" borderWidth="0">
						<g:row>
							<g:customCell>
								<g:Label width="" horizontalAlignment="ALIGN_LEFT" ui:field="txtWelcome"/>
							</g:customCell>
							<g:customCell>
								<g:Label ui:field="lblCurrentMonth"/>
							</g:customCell>
							<g:customCell>
								<g:Hyperlink targetHistoryToken="newHistoryToken" width="" ui:field="cmdLogout"/>
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:Image url="images/13.gif" ui:field="imgProgressBar" width=""/>
							</g:customCell>
							<g:customCell>
								<g:Label/>
							</g:customCell>
							<g:customCell>
								<g:Button text="Afficher/ Cacher le menu" ui:field="button"/>
							</g:customCell>
						</g:row>
					</g:Grid>
					</g:Cell>
				</g:HorizontalPanel>
			</g:Dock>
			<g:Dock direction="WEST" width="10%" height="100%">
				<g:Tree animationEnabled="true" styleName="sideBar" height="" width="" ui:field="treeMenu">
					<g:Button ui:field="cmdHomepage">Accueil</g:Button>
					<g:Button ui:field="cmdTimeInput" text="Saisir des heures des professeurs">Mettre des heures</g:Button>
					<g:TreeItem text="Rapports des heures" state="true">
						<g:Button text="Par départements" ui:field="cmdRptByDept">Par départements</g:Button>
						<g:Button text="Par professeurs" ui:field="cmdExtractData">Par professeurs</g:Button>
						<g:Button ui:field="cmdRptByMonth">Par mois - Compta</g:Button>
					</g:TreeItem>
					<g:TreeItem text="Informations de base" state="true">
						<g:TreeItem text="Professeurs">
							<g:Button text="Liste des professeurs" ui:field="cmdProfs"/>
							<g:Button text="Ajouter un professeur" ui:field="cmdProfsAdd"/>
						</g:TreeItem>
						<g:TreeItem text="Ecoles">
						<g:Button ui:field="cmdEcolePage" text="Liste des écoles">Liste d'écoles</g:Button>
						<g:Button text="Ajouter une école" ui:field="cmdEcoleAdd"/>
					</g:TreeItem>
						<g:TreeItem text="Départements">
							<g:Button ui:field="cmdCours" text="Liste des départements">Liste de cours</g:Button>
							<g:Button ui:field="cmdCoursAdd" text="Ajouter un département">Ajouter un cours</g:Button>
						</g:TreeItem>
						<g:TreeItem text="Types d&apos;heure">
							<g:Button text="Liste des types" ui:field="cmdTypes"/>
							<g:Button ui:field="cmdTypesAdd">Ajouter un type</g:Button>
						</g:TreeItem>
						<g:TreeItem text="Gestion d&apos;accès" state="true">
							<g:Button text="Gestion des utilisateurs" ui:field="cmdUserMgt"/>
							<g:Button ui:field="cmdSettings" text="Bloquer l&apos;accès"/>
						</g:TreeItem>
					</g:TreeItem>
					<g:Button ui:field="cmdContact">Contact</g:Button>
					<g:Image url="images/appengine-silver-120x30.gif"/>
				</g:Tree>
			</g:Dock>
			<g:Dock direction="CENTER" width="">
				<g:FlowPanel ui:field="mainContentPanel" width="" height="" styleName="paneMain"/>
			</g:Dock>
			<g:Dock direction="SOUTH">
				<g:HorizontalPanel width="100%" horizontalAlignment="ALIGN_RIGHT">
					<g:Label text="Copyright 2012 Ecole Lémania"/>
				</g:HorizontalPanel>
			</g:Dock>
		</g:DockPanel>
	</g:FlowPanel>
</ui:UiBinder>
