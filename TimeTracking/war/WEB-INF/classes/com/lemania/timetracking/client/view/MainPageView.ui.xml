<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:FlowPanel width="100%" height="100%">
		<g:DockPanel width="100%" height="100%" spacing="5">
			<g:Dock direction="NORTH">
				<g:HorizontalPanel verticalAlignment="ALIGN_BOTTOM" width="90%" height="54px" spacing="5">
					<g:Image url="images/logo.jpg" width=""/>
					<g:Label width="" horizontalAlignment="ALIGN_RIGHT" ui:field="txtWelcome"/>
					<g:Hyperlink targetHistoryToken="newHistoryToken" width="" ui:field="cmdLogout"/>
					<g:Image url="images/appengine-silver-120x30.gif"/>
				</g:HorizontalPanel>
			</g:Dock>
			<g:Dock direction="WEST" width="10%" height="100%">
				<g:Tree animationEnabled="true" styleName="sideBar" height="600px">
					<g:Button ui:field="cmdHomepage">Accueil</g:Button>
					<g:Button ui:field="cmdTimeInput">Mettre des heures</g:Button>
					<g:TreeItem text="Rapports" state="true">
						<g:Button text="Extraire des heures" ui:field="cmdExtractData"/>
					</g:TreeItem>
					<g:TreeItem text="Informations de base" state="true">
						<g:TreeItem text="Ecoles">
						<g:Button ui:field="cmdEcolePage">Liste d'écoles</g:Button>
						<g:Button text="Ajouter une école" ui:field="cmdEcoleAdd"/>
					</g:TreeItem>
						<g:TreeItem text="Noms de Cours">
							<g:Button ui:field="cmdCours" text="Liste des noms">Liste de cours</g:Button>
							<g:Button ui:field="cmdCoursAdd" text="Ajouter un nom">Ajouter un cours</g:Button>
						</g:TreeItem>
						<g:TreeItem text="Types d&apos;heure">
							<g:Button text="Liste des types" ui:field="cmdTypes"/>
							<g:Button ui:field="cmdTypesAdd">Ajouter un type</g:Button>
						</g:TreeItem>
						<g:TreeItem text="Professeurs" state="true">
							<g:Button text="Liste des professeurs" ui:field="cmdProfs"/>
							<g:Button text="Ajouter un professeur" ui:field="cmdProfsAdd"/>
						</g:TreeItem>
					</g:TreeItem>
					<g:Button ui:field="cmdContact">Contact</g:Button>
				</g:Tree>
			</g:Dock>
			<g:Dock direction="CENTER">
				<g:FlowPanel ui:field="mainContentPanel" width="90%" height="600px" styleName="paneMain"/>
			</g:Dock>
			<g:Dock direction="SOUTH">
				<g:HorizontalPanel horizontalAlignment="ALIGN_RIGHT" width="90%">
					<g:Label text="Copyright 2012 Ecole Lémania"/>
				</g:HorizontalPanel>
			</g:Dock>
		</g:DockPanel>
	</g:FlowPanel>
</ui:UiBinder>
